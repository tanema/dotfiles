port 587
protocol smtp
auth on
from timanema@gmail.com
user timanema@gmail.com
tls on
tls_trust_file ~/.mutt/Equifax_Secure_CA.cert

account timanema.com
host smtp.gmail.com
port 587
protocol smtp
auth on
from me@timanema.com
user me@timanema.com
tls on
tls_trust_file ~/.mutt/Equifax_Secure_CA.cert

account default : gmail.com
